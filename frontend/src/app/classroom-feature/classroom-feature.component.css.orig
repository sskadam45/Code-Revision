@keyframes backgroundClassroom {
  0%   {background-color:#9999CC; left:0px; top:0px;}
  25%  {background-color:#99CCCC; left:200px; top:0px;}
  50%  {background-color:#CC99CC; left:200px; top:200px;}
  75%  {background-color:#CCFFCC; left:0px; top:200px;}
  100% {background-color:#99CCCC; left:0px; top:0px;}
}
